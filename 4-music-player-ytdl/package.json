{
  "name": "4-music-player-ytdl",
  "version": "1.0.0",
  "description": "Example of music bot with discordx",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord-music-bot.git"
  },
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript"
  ],
  "author": "Vijay Meena <indianoceanroleplay@gmail.com> [https://github.com/oceanroleplay]",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord-music-bot/issues"
  },
  "homepage": "https://github.com/oceanroleplay/discord-music-bot#readme",
  "devDependencies": {
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.9.0",
    "@discordx/importer": "^1.1.3",
    "@discordx/music": "^4.0.1",
    "@discordx/pagination": "^2.1.2",
    "discord.js": "^13.7.0",
    "discordx": "^9.6.0",
    "ffmpeg-static": "^5.0.0",
    "libsodium-wrappers": "^0.7.10",
    "reflect-metadata": "^0.1.13"
  }
}
